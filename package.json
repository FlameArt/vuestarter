{
  "name": "appname",
  "version": "1.0.9",
  "scripts": {
    "install/update": "npm i && npm update --save",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "npm run build && vite preview",
    "push and publish": "git push && npm --no-git-tag-version version patch && npm publish",
    "android install": "rmdir android /s /q & npm run build && npx cap add android && npx cap sync && npx capacitor-assets generate --android && copy \"google-services.json\" \"android/app/google-services.json\" && npx cap open android",
    "android update": "npm run build && npx cap sync && npx capacitor-assets generate --android && copy \"google-services.json\" \"android/app/google-services.json\" && npx cap open android",
    "ios": "rmdir ios /s /q & npm run build && npx cap add ios && npx cap sync && npx capacitor-assets generate --ios && npx cap copy ios && cd ios/App/ && pod reintegrate && pod install && cd ./../../ && npx cap open ios"
  },
  "dependencies": {
    "flamerest": "^1.0.98",
    "flameresttable": "^1.0.95",
    "pinia": "^2.1.7",
    "pug": "github:FlameArt/pug-tailwind-only",
    "pug-lexer": "github:FlameArt/pug-lexer-only",
    "sass": "^1.76.0",
    "vue": "^3.4.26",
    "vue-router": "^4.3.2",
    "vuetify": "^3.6.3"
  },
  "devDependencies": {
    "@capacitor-community/file-opener": "^6.0.0",
    "@capacitor/android": "^6.0.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/assets": "^3.0.4",
    "@capacitor/cli": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/device": "^6.0.0",
    "@capacitor/filesystem": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@capacitor/push-notifications": "^6.0.0",
    "@heroicons/vue": "^2.1.3",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/lodash.merge": "^4.6.9",
    "@types/vue-select": "^3.16.8",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@volar/vue-language-plugin-pug": "^1.6.5",
    "@vue/language-plugin-pug": "^2.0.16",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.2.0",
    "eslint-plugin-vue": "^9.25.0",
    "lodash.merge": "^4.6.2",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vue-i18n": "^9.13.1",
    "vue-tsc": "^2.0.16",
    "vue-universal-modal": "^1.1.4"
  },
