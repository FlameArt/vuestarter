{
  "name": "appname",
  "version": "1.0.8",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "npm run build && vite preview",
    "install/update": "npm i && npm update --save",
    "push and publish": "git push && npm --no-git-tag-version version patch && npm publish",
    "android": "rmdir android /s /q & npm run build && npx cap add android && npx cap sync && npx capacitor-assets generate --android && npx cap open android",
    "ios": "rmdir ios /s /q & npm run build && npx cap add ios && npx cap sync && npx capacitor-assets generate --ios && npx cap copy ios && cd ios/App/ && pod reintegrate && pod install && cd ./../../ && npx cap open ios"
  },
  "dependencies": {
    "flamerest": "^1.0.74",
    "pinia": "^2.0.33",
    "pug": "github:FlameArt/pug-tailwind-only",
    "pug-lexer": "github:FlameArt/pug-lexer-only",
    "sass": "^1.58.3",
    "vue": "^3.2.25",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@capacitor/android": "^4.7.0",
    "@capacitor/assets": "^2.0.4",
    "@capacitor/cli": "^4.7.0",
    "@capacitor/core": "^4.7.0",
    "@capacitor/ios": "^4.7.0",
    "@heroicons/vue": "^2.0.16",
    "@types/lodash.merge": "^4.6.7",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@volar/vue-language-plugin-pug": "^1.2.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.35.0",
    "eslint-plugin-vue": "^8.7.1",
    "lodash.merge": "^4.6.2",
    "postcss": "^8.4.21",
    "postcss-import": "^14.1.0",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vue-tsc": "^1.2.0",
    "vue-universal-modal": "^1.1.4"
  },
  "rules": {}
}